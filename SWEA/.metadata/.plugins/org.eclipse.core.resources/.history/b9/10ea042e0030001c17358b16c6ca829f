		bfs(1, 1);
		for (int r = 0; r < 3; r++) {
			for (int c = 0; c < 4; c++) {
				System.out.print(map[r][c] + " ");
			}
			System.out.println();
		}
	}

	static void bfs(int row, int col) {
		Queue<Pos> q = new LinkedList<>();

	}

	static class Pos {
		int r, c;

		public Pos(int r, int c) {
			super();
			this.r = r;
			this.c = c;
		}
	}
