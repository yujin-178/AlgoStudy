import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.Arrays;
import java.util.StringTokenizer;

public class B1197_최소스패닝트리2 {
	static int V, E;
	static int[] head;

	public static void main(String[] args) throws IOException {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		StringTokenizer st = new StringTokenizer(br.readLine());

		V = Integer.parseInt(st.nextToken());
		E = Integer.parseInt(st.nextToken());

		Edge[] eArray = new Edge[E];

		for (int i = 0; i < E; i++) { // ---------------- edge input start
			st = new StringTokenizer(br.readLine());
			int start = Integer.parseInt(st.nextToken());
			int end = Integer.parseInt(st.nextToken());
			int value = Integer.parseInt(st.nextToken());
			eArray[i] = new Edge(start, end, value);
		} // --------------------------------------------- edge input end

		Arrays.sort(eArray);

		makeHead(); // Make-Union

		int cnt = 0;
		long ans = 0;
		for (int i = 0; i < E; i++) { // KRUSKAL
			if (findHead(eArray[i].s) != findHead(eArray[i].e)) { // if not cylce
				cnt++; // cnt increment
				ans += eArray[i].w; // add weight
				head[head[eArray[i].e]] = head[eArray[i].s]; // Union-Set
			}
			if (cnt == V - 1) // KRUSKAL end condition
				break;
		} // end KRUSKAL
		System.out.println(ans);

	}

	static int findHead(int idx) {
		if (idx == head[idx]) // idx와 idx의 머리가 같다면
			return idx; // 리턴해준다.
		// 다르다면
		return head[idx] = findHead(head[idx]); // idx의 head의 head를 찾으러 간다.
		// 최종 결과물은 리턴되면서 각 idx의 head를 갱신한다.
	}

	static void makeHead() {
		head = new int[V + 1]; // 초기 본인의 head를 생성한다.
		for (int i = 0; i <= V; i++)
			head[i] = i; // 처음엔 자기 자신이다.
	}

	static class Edge implements Comparable<Edge> { // interface를 상속한다.
		int s;
		int e;
		int w;

		public Edge(int s, int e, int w) {
			super();
			this.s = s;
			this.e = e;
			this.w = w;
		}

		@Override // 추상화 개념된 개념을 클래스에 맞게 구체화 시킨다.
		public int compareTo(Edge o) { //
			return Integer.compare(this.w, o.w); // Wrapper 클래스의 비교를 사용해서 비교했다. 왜?였지...?

		}

	}
}
