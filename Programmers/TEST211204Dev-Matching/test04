-- 코드를 입력하세요
SELECT ID, NAME,SUM(PRICE)  
FROM PLACES P LEFT OUTER JOIN SCHEDULES S 
ON P.ID = S.PLACE_ID 
WHERE ( YEAR(S.SCHEDULED_AT) = 2019 AND MONTH(S.SCHEDULED_AT) = 1 AND DAY(S.SCHEDULED_AT) = 15)
OR ( YEAR(S.SCHEDULED_AT) = 2019 AND MONTH(S.SCHEDULED_AT) = 1 AND DAY(S.SCHEDULED_AT) = 16)
OR ( YEAR(S.SCHEDULED_AT) = 2019 AND MONTH(S.SCHEDULED_AT) = 1 AND DAY(S.SCHEDULED_AT) = 17)
GROUP BY ID, NAME
HAVING COUNT(PRICE ) = 3